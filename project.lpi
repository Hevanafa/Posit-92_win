<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <PathDelim Value="\"/>
    <General>
      <Flags>
        <MainUnitHasCreateFormStatements Value="False"/>
        <MainUnitHasTitleStatement Value="False"/>
        <MainUnitHasScaledStatement Value="False"/>
      </Flags>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="project"/>
      <UseAppBundle Value="False"/>
      <ResourceType Value="res"/>
    </General>
    <BuildModes>
      <Item Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <RunParams>
      <FormatVersion Value="2"/>
    </RunParams>
    <Units>
      <Unit>
        <Filename Value="project.lpr"/>
        <IsPartOfProject Value="True"/>
      </Unit>
      <Unit>
        <Filename Value="units\SDL\sdl2.pas"/>
        <IsPartOfProject Value="True"/>
      </Unit>
      <Unit>
        <Filename Value="main.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Main"/>
      </Unit>
      <Unit>
        <Filename Value="units\vga.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="VGA"/>
      </Unit>
      <Unit>
        <Filename Value="units\keyboard.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Keyboard"/>
      </Unit>
      <Unit>
        <Filename Value="units\bitmap.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Bitmap"/>
      </Unit>
      <Unit>
        <Filename Value="units\timing.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Timing"/>
      </Unit>
      <Unit>
        <Filename Value="units\renderer.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Renderer"/>
      </Unit>
      <Unit>
        <Filename Value="units\events.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Events"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <PathDelim Value="\"/>
    <Target>
      <Filename Value="project"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)"/>
      <OtherUnitFiles Value="units;units\SDL"/>
      <UnitOutputDirectory Value="lib\$(TargetCPU)-$(TargetOS)"/>
    </SearchPaths>
    <Linking>
      <Debugging>
        <DebugInfoType Value="dsDwarf3"/>
      </Debugging>
    </Linking>
  </CompilerOptions>
  <Debugging>
    <Exceptions>
      <Item>
        <Name Value="EAbort"/>
      </Item>
      <Item>
        <Name Value="ECodetoolError"/>
      </Item>
      <Item>
        <Name Value="EFOpenError"/>
      </Item>
    </Exceptions>
  </Debugging>
</CONFIG>
